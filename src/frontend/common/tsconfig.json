{
	"compilerOptions": {
		"out": "../../../public/javascripts/common.js",
		"allowJs": true,
		"target": "es3",
		"noImplicitReturns": true,
		"allowUnreachableCode": false,
		"newLine": "LF",
		"sourceMap": false,
		"module": "none",
		"preserveConstEnums": true,
		"removeComments": true
	},
	"include": [
		"./polyfill.ts",
		"./common.ts",
		"../*.d.ts",
		"../../shared/**/*.d.ts"
	]
}
